{
  "manifest_version": 2,
  "name": "Emoji Substitution",
  "description": "Replaces words with emojis.",
  "version": "0.1.0",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/emojimap.js", "js/index.js"]
    }
  ]
}
